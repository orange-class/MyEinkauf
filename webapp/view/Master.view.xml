<mvc:View 
	controllerName="qstMyEinkauf.controller.Master"
	xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:core="sap.ui.core" 
	xmlns:mvc="sap.ui.core.mvc" 
	xmlns="sap.m" >
	<Page
		title="Meine Einkaufslisten"> <!-- SpÃ¤ter ggf. an Namen des eingeloggten Nutzers anpassen -->
		<List id="el" 
			items="{/orders}"
			mode="SingleSelectMaster"
			selectionChange="onSelectionChange"> 
			<!-- id wird nirgends verwendet bisher 
			items muss an unser Modell angepasst werden-->
			<items>
				<StandardListItem title="{orderName}" 
					type="Active"
					press="onSelectionChange"/>
				<!-- title muss an unser Modell angepasst werden! -->
			</items>
		</List>
	</Page>
</mvc:View>