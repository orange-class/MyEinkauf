<mvc:View 
	xmlns="sap.m"
	xmlns:layout="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:t="sap.ui.table"
	xmlns:core="sap.ui.core"
	controllerName="qstMyEinkauf.controller.ListSettings"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<Page
		title="{beispiel>orderName} von {beispiel>owner}"
		showNavButton="true"
		navButtonPress="onNavBack"
		contentOnlyBusy="true"
		titleAlignment="Start">
		<content>
			<t:Table rows="" id="table_settings" > 
	                   <t:columns>
	                   	<t:Column>
							<t:label>
								<Label text="Name" id="label_list_idr"/>
							</t:label>
							<t:template>
								<Text text="Foo" id="text_list_idr"/>
							</t:template>
						</t:Column> 
						
						<t:Column>
							<t:label>
								<Label text="Rolle" id="label_item_id"/>
							</t:label>
							<t:template>
								<ComboBox  id="box_master">
	
			                         <items>
	                                       <core:ListItem text="Ansehen" key="item_zeit" id="combo_ansehen"/>
			                               <core:ListItem text="Bearbeiten" key="item_ersteller" id="combo_bearbeiten"/>
			                               <core:ListItem text="Teilen" key="item_zeit" id="combo_teilen"/>
	
			                          </items>
	
			                    </ComboBox>
							</t:template>
						</t:Column> 
						
						</t:columns>
		
					</t:Table>
		</content>
	</Page>
</mvc:View>
