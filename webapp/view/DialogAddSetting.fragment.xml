<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:html="http://www.w3.org/1999/xhtml">
	<Dialog id="DialogAddSetting" title="Freigabe">
		<Label text="Name: "/>
		<Input id="input_name_dialog" width="50%"/>
		<html:br/>
		<Label text="Zugriffsrecht: "/>
		<ComboBox id="box_dialog" selectionChange="onSelectionChangeAddSetting"
			value="{= ${EinkaufBackend>UserRights} === 'A' ? 'Ansehen' : ${beispiel>UserRights} === 'B' ? 'Bearbeiten' : 'Teilen' }">
			<core:Item text="Ansehen" key='A' id="item_a"/>
			<core:Item text="Bearbeiten" key='B' id="item_b"/>
			<core:Item text="Teilen" key='T' id="item_t"/>
		</ComboBox>
		<beginButton>
			<Button text="BestÃ¤tigen" press="closeDialogAddSetting"/>
		</beginButton>
		<endButton>
			<Button text="Abbrechen" press="cancelDialogAddSetting"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>